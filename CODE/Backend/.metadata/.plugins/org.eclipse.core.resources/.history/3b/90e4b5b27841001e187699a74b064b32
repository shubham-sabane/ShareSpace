package Entity;

public class Property {
	
	private int id;
	private Address address;
	private String addLine1;
	private double area;
	private double price;
	private Amenity amenities;
	private PropertyFeatures features;
	
	public Property() {
		super();
	}

	public Property(int id, Address address, String addLine1, double area, double price, Amenity amenities,
			PropertyFeatures features) {
		super();
		this.id = id;
		this.address = address;
		this.addLine1 = addLine1;
		this.area = area;
		this.price = price;
		this.amenities = amenities;
		this.features = features;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public Address getAddress() {
		return address;
	}

	public void setAddress(Address address) {
		this.address = address;
	}

	public String getAddLine1() {
		return addLine1;
	}

	public void setAddLine1(String addLine1) {
		this.addLine1 = addLine1;
	}

	public double getArea() {
		return area;
	}

	public void setArea(double area) {
		this.area = area;
	}

	public double getPrice() {
		return price;
	}

	public void setPrice(double price) {
		this.price = price;
	}

	public Amenity getAmenities() {
		return amenities;
	}

	public void setAmenities(Amenity amenities) {
		this.amenities = amenities;
	}

	public PropertyFeatures getFeatures() {
		return features;
	}

	public void setFeatures(PropertyFeatures features) {
		this.features = features;
	}

	
	
}
